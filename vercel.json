{
  "version": 2,
  "builds": [{ "src": "api/index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/connect/linkedin", "dest": "/api/index.js" },
    { "src": "/unipile/notify", "dest": "/api/index.js" },
    { "src": "/(success|failure)", "dest": "/public/$1.html" },
    { "src": "/", "dest": "/public/index.html" }
  ]
}
